CO2_Cell {
  HBox { +-; Title: "CO2 Cell"; -+ };
  HP: (CCelHP,7) V;
  LP: (CCelLP,7) V;
  P: (CCelP,7) V;
  PSt: (CCelPSt,6) V;
  1T: (CCel1T,6) C;
  2T: (CCel2T,6) C;
  CT: (CCelCT,7) V;
  TSt: (CCelTSt,6) V;
  HC: (CCell_HC_DS,3);
  {
    Prs_Reg: (CPrs_Reg_DS,3);
    Ex_Vlv: (CGas_Ex_Vlv_DS,3);
    Gas_Vlv: (CGas_Vlv_DS,3);
  }
}

MM_Cell {
  HBox { +-; Title: "Mini CH4 Cell"; -+ };
  HP: (MCelHP,7) V;
  LP: (MCelLP,7) V;
  P: (MCelP,7) V;
  PSt: (MCelPSt,6) V;
  1T: (MCel1T,6) C;
  2T: (MCel2T,6) C;
  CT: (MCelCT,7) V;
  TSt: (MCelTSt,6) V;
  HC: (MCell_HC_DS,3);
  {
    Prs_Reg: (MPrs_Reg_DS,3);
    Ex_Vlv: (MGas_Ex_Vlv_DS,3);
    Gas_Vlv: (MGas_Vlv_DS,3);
  }
}

I_Cell {
  HBox { +-; Title: "ICOS Cell"; -+ };
  HP: (ICelHP,7) V;
  LP: (ICelLP,7) V;
  P: (ICelP,7) V;
  PSt: (ICelPSt,6) V;
  1T: (ICel1T,6) C;
  2T: (ICel2T,6) C;
  CT: (ICelCT,7) V;
  TSt: (ICelTSt,6) V;
  HC: (ICell_HC_DS,3);
  {
    Prs_Reg: (IPrs_Reg_DS,3);
