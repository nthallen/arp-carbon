# QCLI_M.qcli
# Generic Waveforms for QCLI_M
QCLIConfig StandardxP33;
Digitizer SSP;
Prefix QCLI_M;

ICOS Waveform M3Df_800Hz {
  SampleRate = 50 MHz/28;
  Ncoadd = 799;
  Tz = 5%;
  Ramp from 650 mA to 780 mA in 1 msec;
}

ICOS Waveform QCLI_M_ConstantI {
  Tz = 10 msec;
  Iz = 700 mA;
  SampleRate = 10 MHz;
  dI/dt = 0 A/sec;
}

Ringdown Waveform M3Df_ring750 {
  SampleRate = 25 MHz;
  Toff = 80 usec;
  Ton = 200 usec;
  Tpre = 2 usec;
  I = 750 mA;
}
