# Partition 16: Pumpkin Troll
#   Now responsible for Circuit 3 control
#----------------------------------------------------------------
Partition

%{
 #define PUMP_TEMP_HOT 50
 #define PUMP_TEMP_OK(x) (x < PUMP_TEMP_HOT)
 int Pump_Ground_Power = 0;
%}

State Pump_Init {
    > CO2 Exhaust Valve Closed
    > ISO Exhaust Valve Closed
 +1 > MINI Scroll Pump Off
 +1 > Circuit3 Off
 +3 Hold until (convert(PM1V1) > 20) or 5
    else Validate Pump_Low_Battery;
 +1 > Circuit3 On
    Hold until (Pump_Ground_Power || convert(PM1V1) > 26) or 3
    else Validate Pump_Circuit3_Fail;
    Validate Inverter_Start;
    Depending on (Record) { Validate Pump_Start; }
}

State Pump_Start {
    Depending on (Shutdown) { Validate Pump_Shutdown; }
    Hold until valid (Inverter_On) or 5
      else Validate Pump_Init;
 +5 > MINI Scroll Pump On
    Hold until valid (Inverter_Off) or 5
      else Validate Pump_Is_On;
 +1 > MINI Scroll Pump Off
 +1 Hold until (Pump_Ground_Power || convert(PM1V1) > 26) or 3
      else Validate Pump_Circuit3_Fail;
 +1 > Circuit3 Off
 +1 Validate Inverter_Start;
    Hold until valid (Inverter_On_Battery) or 10
      else Validate Pump_Inverter_Timeout;
 +1 > MINI Scroll Pump On
    Hold until valid (Inverter_Off) or 5
      else Validate Pump_Is_On;
 +1 { msg(2, "Inverter failed with and without circuit 3. Retrying"); }
 +1 Validate Pump_Init;
}

State Pump_Circuit3_Fail {
    { msg(2, "Bus 3 Power Failure"); }
    > Panel Display Text \\nBus 3 Power Failure
    Hold until (Pump_Ground_Power || convert(PM1V1) > 26);
    Validate Pump_Init;
}

State Pump_Low_Battery {
 +1 { msg(2, "Low Battery Voltage: %.1lf", convert(PM1V1)); }
 +1 > Panel Display Text \\nLow Battery Voltage
