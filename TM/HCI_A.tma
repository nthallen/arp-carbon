    Validate Pump_Init;
}

State Pump_Inverter_Timeout {
    { msg(2, "Unexpected Inverter Timeout on turn on"); }
    Validate Pump_Init;
}

State Pump_Is_On {
     Depending on (Shutdown) { Validate Pump_Shutdown; }
     Depending on (Inverter_Off) { Validate Pump_Inverter_Off; }
     Depending on (Inverter_Fail) { Validate Pump_Inverter_Off; }
  +1 > Circuit3 On
  +1 > ISO Exhaust Valve Open
  +5 > CO2 Exhaust Valve Open
     Validate Pump_Watch;
}

State Pump_Watch Nolog {
      Depending on (Shutdown) { Validate Pump_Shutdown; }
      Depending on (Inverter_Off) { Validate Pump_Inverter_Off; }
      Depending on (Inverter_Fail) { Validate Pump_Inverter_Off; }
      hold until ( !PUMP_TEMP_OK(convert(MPmpT)) );
      hold until ( PUMP_TEMP_OK(convert(MPmpT)) ) or 5
      else Validate Pump_Hot;
      Validate Pump_Watch;
