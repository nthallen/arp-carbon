   +1 Hold until valid (Pump_Off);
   +1 hold until ( PUMP_COLD_OK(convert(CPmpT)) &&
         PUMP_COLD_OK(convert(MPmpT)) );
   +1 > Coolant Pump Off
   +1 Validate Pump_Cool_Is_Shutdown;
}

