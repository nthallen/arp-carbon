# -----------------------------------------------------------------------------
# Partition 18: Luggage Bay Temperature Control
# Throttle Valve Control and Space Heater Control
# -----------------------------------------------------------------------------
Partition

%{
  #define HEATER_HOT 2.0
  #define HEATER_TEMP_OK(x) (x<HEATER_HOT)
  #define BAY_HOT 25
  #define BAY_TEMP_OK(x) (x<BAY_HOT)
  #define LBHtrMaxSP 1.9
  
  double LBT_sum = 0.;
  double LBTGp = 900.;
  double LBTGi = 80.; /* requires 1 minute to completely unwind at T_diff of 1.0 */
  double LBT_set = 25.;
  #define LBT_MAX_CAL_BITS 48000.
%}

State LuggageBay_TCtrl_Init {
    Depending on (Shutdown) { Validate LuggageBay_TCtrl_Shutdown; }
    Depending on (Record) { Validate LuggageBay_TCtrl_PI_Init; }
   +1 > HK Space Htr Ctl Off
   +1 > drive Indexer Throttle Valve in 48000
    Hold until ((IOSwS & 3) == 3) ;
    Validate LuggageBay_TCtrl_PI_Init; 
} 

State LuggageBay_TCtrl_PI_Init {
  { LBT_sum = 0; LBTGp = 300; LBTGi = 80; }
  +2 Validate LuggageBay_TCtrl_PI;
}

State LuggageBay_TCtrl_PI {
  Depending on (Shutdown) { Validate LuggageBay_TCtrl_Shutdown; }
  { double LBT, LBT_diff, dstep; 
    LBT = (convert(CRv5T)+convert(MRv5T))/2;
    LBT_diff = LBT_set - LBT;
    LBT_sum += LBT_diff * LBTGi;
    if ( LBT_sum > LBT_MAX_CAL_BITS )
      LBT_sum = LBT_MAX_CAL_BITS;
    else if ( -LBT_sum > LBT_MAX_CAL_BITS )
      LBT_sum = -1. * LBT_MAX_CAL_BITS;
    dstep = LBT_diff * LBTGp + LBT_sum;
    if (dstep > LBT_MAX_CAL_BITS)
      dstep = LBT_MAX_CAL_BITS;
    else if (-dstep > LBT_MAX_CAL_BITS)
      dstep = -1. * LBT_MAX_CAL_BITS;
    if (dstep > 0) { /* Temp below SetPoint, Heating required */
      if (!HEATER_TEMP_OK(convert(SpHCT))) {
        Validate LuggageBay_TCtrl_Hot;
      } else {
        if (!(TVStat & 1))
          ci_sendfcmd(2, "Drive Indexer Throttle Valve In 48000\n");
        dstep = LBHtrMaxSP * dstep / LBT_MAX_CAL_BITS;
        ci_sendfcmd(2, "HK Set Space Heater Temp Setpoint %.2f\n", dstep);
        if (!(SpHt_HC_DS))
          ci_sendfcmd(2, "HK Space Htr Ctl On\n");
      }
    } else { /* Temp above SetPoint, Cooling required */
      unsigned step;
      step = -dstep;
      if (SpHt_HC_DS)
        ci_sendfcmd(2, "HK Space Htr Ctl Off\n");
      ci_sendfcmd(2, "Drive Indexer Throttle Valve To %u\n", step);
    }
  }
}

State LuggageBay_TCtrl_Hot {
    Depending on (Shutdown) { Validate LuggageBay_TCtrl_Shutdown; }
 +1 > HK Space Htr Ctl Off
    Hold until (convert(SpHCT) < 1.4);
    Validate LuggageBay_TCtrl_PI_Init;
} 

State LuggageBay_TCtrl_Shutdown {
     > HK Space Htr Ctl Off
     > Drive Indexer Throttle Valve In 48000
} 

