Experiment=HCI
HomeDir=/home/hci
SUBBUSD=serusb
SUBBUSD_DELAY=3
NETWORK_DELAY=0
FlightNode=hci
SCRIPT_OVERRIDE=script
EXP_NODES="hci hcigse"
PICKFILE='eval echo runfile.`sbswitch 3`'
